        	function janrainCaptureWidgetOnLoad() {
			
			janrain.capture.ui.start();
			janrain.events.onCaptureLoginSuccess.addHandler(function(result){
				janrain.capture.ui.modal.close();
				if (window.console && window.console.log) console.log(result);

				var _captureUUID		= encodeURIComponent( result.userData.uuid );
				var _captureDisplayName	= encodeURIComponent( result.userData.displayName );
				var _captureEmail		= encodeURIComponent( result.userData.email );

				var _returnUrl			= encodeURIComponent( window.location );
		
				var visitUrl	= 'http://forums.oreilly.com' + '/interface/janrain/login.php?' +
					'uid=' + _captureUUID + '&' +
					'name=' + _captureDisplayName + '&' +
					'email=' + _captureEmail + '&' +
					'return=' + _returnUrl;

					window.location	= visitUrl;
				
			});
			janrain.events.onCaptureSessionFound.addHandler(function(result){
				janrain.capture.ui.modal.close();
				if (window.console && window.console.log) console.log(result);
			});
			janrain.events.onCaptureRegistrationSuccess.addHandler(function(result){
				janrain.capture.ui.modal.close();
				if (window.console && window.console.log) console.log(result);

				var _captureUUID		= encodeURIComponent( result.userData.uuid );
				var _captureDisplayName	= encodeURIComponent( result.userData.displayName );
				var _captureEmail		= encodeURIComponent( result.userData.email );

				var _returnUrl			= encodeURIComponent( window.location );
		
				var visitUrl	= 'http://forums.oreilly.com' + '/interface/janrain/login.php?' +
					'uid=' + _captureUUID + '&' +
					'name=' + _captureDisplayName + '&' +
					'email=' + _captureEmail + '&' +
					'return=' + _returnUrl;

					window.location	= visitUrl;
			});
			janrain.events.onCaptureSessionEnded.addHandler(function(result){

				$$('#user_navigation a').each( function(elem){
				
					if( /do=logout/ig.test( $(elem).href ) )
					{
							window.location	= $(elem).href;
					}
						
				});							
			});
			janrain.events.onCaptureScreenShow.addHandler(function(result){
				if (result.screen == "returnTraditional") {
					janrainReturnExperience();
				}
			});

			/* Force the login UI to launch if we are viewing the login page (i.e. by opening in a new window or manually visiting URL) */
			if( /section=login/ig.test( window.location ) && /module=global/ig.test( window.location ) )
			{
				if( $('login_form') )
				{
					$('login_form').hide();
				}

				janrain.capture.ui.modal.open();
			}

			/* Force the register UI to launch if we are viewing the register page (i.e. by opening in a new window or manually visiting URL) */
			if( /section=register/ig.test( window.location ) && /module=global/ig.test( window.location ) && !/do=complete_login/ig.test( window.location ) )
			{
				if( $('register') )
				{
					$('register').hide();
				}

				$$('.ipsSteps').each( function(elem){ $(elem).hide(); });

				janrain.capture.ui.modal.open();
			}
		}
		
